# Apple OAuth Configuration (if using Apple Sign In)
APPLE_CLIENT_ID=your_apple_client_id
SUPABASE_AUTH_EXTERNAL_APPLE_SECRET=your_apple_secret
APPLE_REDIRECT_URI=your_apple_redirect_uri
APPLE_AUTH_URL=your_apple_auth_url

# Other Configuration
EXPO_PUBLIC_APP_URL=http://localhost:3000 